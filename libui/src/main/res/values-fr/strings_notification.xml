<resources>
    <string name="notification_on_text">Notifications</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Quand activé, %s affichera une notification à chaque fois qu\'une publicité est bloquée.</string>

    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Pub bloquée</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Ne pas afficher</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Liste blanche</string>

    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Fait! Les changements peuvent prendre quelques secondes</string>

    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s n\'affichera pas les notifications de publicités.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s n\'affichera pas les notifications à chaque fois qu\'une publicité est bloquée.</string>

    <string name="notification_keepalive_text">Maintenir actif</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Si activé, %s utilisera des techniques qui l\'aide à resister au nettoyage de la mémoire.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s n\'est plus protégé contre l\'économiseur de batterie ou de mémoire.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s se protégera désormais contre sa désactivation par le système.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Une pub bloquée</item>
        <item quantity="other">%d pubs bloquées</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Dernier: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">encore rien</string>
</resources>
